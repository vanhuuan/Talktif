@{
    Layout = "~/Views/Shared/_AdminLayoutPage.cshtml";
    ViewData["Title"] = "Setting";
    <link rel="stylesheet" href="../css/AdminSetting.css" />
}
<div class="container-fluid">
    <div class="row row-wrapper">
        <div class="col-lg-6 col-md-6 col-sm-6 col-xs-6 tablet">
            <div class="user-profile">
                <h2 class="title">User Profile</h2>
                <form action="#" class="form">
                    <div class="user-info">
                        <span class="details">UserName :</span>
                        <div class="name">
                            <input id="username" type="text" value="" placeholder="Username" required disabled />
                            <i class="fas fa-edit username"></i>
                        </div>
                    </div>
                    <div class="user-info">
                        <span class="details">Email :</span>
                        <input id="email" type="email" value="" placeholder="Email" required disabled />
                    </div>
                    <div class="user-info">
                        <span class="details">Password :</span>
                        <div class="password">
                            <input id="password" type="password" value="" placeholder="Password" required disabled />
                            <i class="fas fa-edit password"></i>
                        </div>
                    </div>
                    <div class="user-info">
                        <span class="details">Confirm Password :</span>
                        <input id="confirmpassword" type="password" value="" placeholder="Confirm Password" required
                            disabled />
                    </div>
                    <div class="submit"><input type="submit" value="Change" class="Update-Profile" /></div>
                </form>
            </div>
        </div>
    </div>
</div>